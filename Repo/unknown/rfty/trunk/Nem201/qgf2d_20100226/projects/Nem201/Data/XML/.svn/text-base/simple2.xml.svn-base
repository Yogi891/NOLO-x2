<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">

<bulletml type="horizontal"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label = "top">
	<fire>
		<direction type="absolute">270</direction>
		<bulletRef label="shoot"/>
	</fire>
</action>

<bullet label="shoot">
	<action>
<repeat><times>6</times>
<action>
	<repeat><times>3</times>
		<action>
		<wait>10</wait>
		<fire>
			<direction type="aim">0</direction>
			<speed>3</speed>
			<bullet>
				<action>
					<changeDirection>
						<direction type="relative">90</direction>
						<term>60</term>
					</changeDirection>
				</action>
			</bullet>
		</fire>
		</action>
	</repeat>
<wait>30</wait>
</action>
</repeat>
	</action>

</bullet>

</bulletml>